<h5>Showing all API Keys</h5>
<div class="row">
  <div class="col-sm-2">
    <h2>For</h2>
  </div>
  <div class="col-sm-2">
    <h2>Key</h2>
  </div>
  <div class="col-sm-2">
    <h2>Get Requests</h2>
  </div>
  <div class="col-sm-2">
    <h2>Post Requests</h2>
  </div>
  <div class="col-sm-2">
    <h2>Search Requests</h2>
  </div>
  <div class="col-sm-2">
    <h2>Action</h2>
  </div>
</div>
<hr>
<% @apikeys.each do |q| %>
<div class="row">
  <div class="col-sm-2">
    <h4 class='wrap'><%= q.for %></h4>
  </div>
  <div class="col-sm-2">
    <p class='wrap'><%= q.key %></p>
  </div>
  <div class="col-sm-2">
    <h4><%= q.get_counter %></h4>
  </div>
  <div class="col-sm-2">
    <h4><%= q.post_counter %></h4>
  </div>
  <div class="col-sm-2">
    <h4><%= q.search_counter %></h4>
  </div>
  <div class="col-sm-2">
    <%= link_to "Delete", apikey_path(q), method: :delete, data: {confirm: "This is final, it WILL be erased..."}, class: "btn btn-danger btn-xs" %>
  </div>
</div>
<hr>
<% end  %>
